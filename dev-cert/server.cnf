[req]
default_bits = 2048
prompt = no
default_md = sha256
req_extensions = req_ext
distinguished_name = dn

[dn]
CN = dev.myubuntu.com

[req_ext]
subjectAltName = @alt_names

[alt_names]
DNS.1 = localhost
DNS.2 = dev.myubuntu.com
DNS.3 = myubuntu.com
DNS.4 = dev.antsentinel.com
DNS.5 = test.antsentinel.com
IP.1 = 127.0.0.1
IP.2 = 192.168.1.176
IP.3 = 192.168.1.198
